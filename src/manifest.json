{
	"manifest_version": 2,

	"name": "Tomcat site manager",
	"description": "Create, reload and remove Tomcat virtual hosts",
	"version": "3.1.3",

	"browser_action": {
		"default_title": "Manage virtual hosts",
		"default_popup": "index.html"
	},

	"icons": {
    "16": "gfx/icon16.png",
    "48": "gfx/icon48.png",
    "128": "gfx/icon128.png"
  },

	"permissions": [
		"tabs",
		"http://localhost/*"
  ]
}
